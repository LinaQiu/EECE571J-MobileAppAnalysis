<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lina.ubc.sensitivitytestallinone">

    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".FieldSensitivity"
            android:label="@string/title_activity_field_sensitivity">
        </activity>
        <activity android:name=".ObjectSensitivity">
        </activity>
        <activity android:name=".ContextSensitivity">
        </activity>
        <activity android:name=".TypeSensitivity">
        </activity>
        <activity
            android:name=".FlowSensitivity"
            android:label="@string/title_activity_flow_sensitivity">
        </activity>
        <activity android:name=".AmandroidSourceSinkTest">
        </activity>
        <activity android:name=".PathSensitivity">
        </activity>
    </application>

</manifest>